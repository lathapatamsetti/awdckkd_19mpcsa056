import java.util.*;
class minimumelement
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int size,i;
System.out.print("enter array size");
size=sc.nextInt();
int ar[]=new int[size];
System.out.print("enter array elements");
for(i=0;i<size;i++)
{
ar[i]=sc.nextInt();
}
int min=ar[0];
for(i=1;i<size;i++)
{
if(ar[i]<min);
}
System.out.println("minimum element is" +min);
}
}




