import java.util.*;
class arraylist1
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int n;
LinkedList<Integer> x=new LinkedList<Integer>();
System.out.println("enter n value");
n=sc.nextInt();
for(int i=0;i<n;i++)
{
x.add(sc.nextInt());
}
for(Integer i:x)
{
System.out.print(i);
}
x.remove(3);
System.out.print(x);
}}








